<section>
    <h2>Posts</h2>
    {{#each posts}}
    <article class='post'>
        <header>
            <h3>{{title}}</h3>
            <a href='/posts/{{slug}}'>{{datetime datePublished "dS mmmm yyyy, h:MMtt"}}</a>
        </header>
        {{{emojify content.body.html}}}
    </article>
    {{/each}}
</section>